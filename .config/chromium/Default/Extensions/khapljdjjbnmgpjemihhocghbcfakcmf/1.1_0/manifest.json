{
   "action": {
      "default_popup": "index.html"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "https://*.skool.com/*" ]
   } ],
   "description": "Save Skool posts with a click and view your bookmarks in one place.",
   "host_permissions": [ "https://*.skool.com/*", "https://skoopcrm.vercel.app/*" ],
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Skool Bookmarks",
   "permissions": [ "activeTab", "storage", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1"
}
